{
  "name": "react-redux-sandbox",
  "version": "0.0.0",
  "description": "React-Redux sandbox",
  "main": "main.js",
  "dependencies": {
    "classnames": "^2.2.2",
    "express": "^4.13.3",
    "node-jsx": "^0.13.3",
    "pm2": "^0.15.10",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "expect": "^1.13.4",
    "mocha": "^2.3.4"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm test -- --watch",
    "build": "npm run js",
    "js": "npm run babel",
    "babel": "babel src --out-dir build && cp src/index.html build",
    "bundle": "browserify build/index.js -o public/bundle.js",
    "start": "pm2 start --no-daemon"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/takasing/react-redux-sandbox.git"
  }
}
